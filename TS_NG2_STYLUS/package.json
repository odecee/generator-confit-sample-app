{
  "name": "ts-ng-2-stylus",
  "version": "0.0.0",
  "description": "A description of my awesome package",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/entity/repo"
  },
  "scripts": {
    "pre-release": "npm-run-all verify test:coverage build test:system",
    "build": "cross-env NODE_ENV=production npm-run-all clean:prod build:prod",
    "build:dev": "webpack-dev-server --progress --config config/webpack/dev.webpack.config.js --hot",
    "build:prod": "webpack -p --progress --config config/webpack/prod.webpack.config.js",
    "build:serve": "npm-run-all build serve:prod",
    "build:stats": "webpack -p --progress --config config/webpack/prod.webpack.config.js -j > stats.json",
    "clean:dev": "rimraf dev/",
    "clean:prod": "rimraf dist/",
    "dev": "cross-env NODE_ENV=development npm-run-all clean:dev --parallel verify:watch build:dev serve:dev",
    "docs:_publish": "node config/docs/publish.js",
    "docs:build": "cross-env NODE_ENV=production webpack -p --progress --config config/docs/swanky.webpack.config.js --colors",
    "docs:build:serve": "npm-run-all docs:build docs:serve",
    "docs:dev": "cross-env NODE_ENV=development node config/docs/serve.dev.js",
    "docs:postpublish": "node config/docs/postpublish.js",
    "docs:prepublish": "node config/docs/prepublish.js",
    "docs:publish": "npm-run-all docs:prepublish docs:build docs:_publish docs:postpublish",
    "docs:serve": "http-server webdocs/ -o",
    "serve:dev": "echo Starting DEV server",
    "serve:prod": "npm run serve:prod:https",
    "serve:prod:http": "serve -C -p 3000 dist/",
    "serve:prod:https": "serve-https -p 3000 -d dist/ --servername localhost",
    "start": "npm run dev",
    "test": "npm run test:unit",
    "test:coverage": "npm run test:unit:once",
    "test:system": "cross-env NODE_ENV=test protractor config/testSystem/protractor.conf.js",
    "test:unit": "cross-env BABEL_ENV=test NODE_ENV=test karma start ./config/testUnit/karma.conf.js",
    "test:unit:debug": "cross-env NODE_ENV=test karma start ./config/testUnit/karma.conf.js --debug",
    "test:unit:debug:once": "cross-env NODE_ENV=test karma start --singleRun=true ./config/testUnit/karma.conf.js --debug",
    "test:unit:once": "cross-env BABEL_ENV=test NODE_ENV=test karma start --singleRun=true ./config/testUnit/karma.conf.js",
    "test:visual": "cross-env NODE_ENV=test cd node_modules/backstopjs && npm run test -- --configPath=../../config/testVisualRegression/backstop.config.js",
    "test:visual:ref": "cross-env NODE_ENV=test cd node_modules/backstopjs && npm run reference -- --configPath=../../config/testVisualRegression/backstop.config.js",
    "upload-coverage": "cat reports/lcov/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "verify": "npm-run-all verify:js verify:css --silent",
    "verify:css": "stylint -c config/verify/.stylintrc src/ && echo ✅ verify:css success",
    "verify:css:watch": "chokidar 'src/**/*.styl' -c 'npm run verify:css' --initial --silent",
    "verify:js": "tslint -c config/verify/tslint.json \"src/**/*.ts\" \"unitTest/**/*.ts\" \"config/**/*.ts\" && echo ✅ verify:js success",
    "verify:js:watch": "chokidar 'src/**/*.ts' 'unitTest/**/*.ts' 'config/**/*.ts' -c 'npm run verify:js' --initial --silent",
    "verify:watch": "npm-run-all --parallel verify:js:watch verify:css:watch --silent"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "config/release/commitMessageConfig.js"
    },
    "ghooks": {
      "pre-push": "npm-run-all verify test:coverage --silent",
      "commit-msg": "./node_modules/cz-customizable-ghooks/lib/index.js $2"
    }
  },
  "dependencies": {
    "@angular/common": "2.1.1",
    "@angular/compiler": "2.1.1",
    "@angular/core": "2.1.1",
    "@angular/platform-browser": "2.1.1",
    "@angular/platform-browser-dynamic": "2.1.1",
    "@angular/router": "3.1.1",
    "core-js": "2.4.1",
    "rxjs": "5.0.0-beta.12",
    "ts-helpers": "1.1.2",
    "zone.js": "0.6.26"
  },
  "devDependencies": {
    "@types/jasmine": "2.2.34",
    "@types/node": "6.0.46",
    "@types/selenium-webdriver": "2.44.29",
    "@types/source-map": "0.1.29",
    "@types/uglify-js": "2.0.27",
    "@types/webpack": "1.12.35",
    "angular2-template-loader": "0.6.0",
    "autoprefixer": "6.4.1",
    "awesome-typescript-loader": "2.2.4",
    "babel-eslint": "6.1.2",
    "backstopjs": "1.3.5",
    "chokidar-cli": "1.2.0",
    "coveralls": "2.11.15",
    "cross-env": "3.1.3",
    "css-loader": "0.24.0",
    "cz-customizable": "4.0.0",
    "cz-customizable-ghooks": "1.3.0",
    "eslint": "7.8.1",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "0.9.0",
    "gh-pages": "0.11.0",
    "ghooks": "1.3.2",
    "glob": "7.0.5",
    "html-loader": "0.4.3",
    "html-webpack-plugin": "2.22.0",
    "http-server": "0.9.0",
    "istanbul-instrumenter-loader": "0.2.0",
    "jasmine-core": "2.4.1",
    "jasmine-reporters": "2.1.1",
    "jasmine-spec-reporter": "2.4.0",
    "js-yaml": "3.6.1",
    "karma": "1.2.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-coverage": "1.1.1",
    "karma-jasmine": "1.0.2",
    "karma-junit-reporter": "1.1.0",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-sourcemap-loader": "0.3.7",
    "karma-threshold-reporter": "0.1.15",
    "karma-webpack": "1.8.0",
    "latest-version": "2.0.0",
    "npm-run-all": "3.1.0",
    "phantomjs-polyfill": "0.0.2",
    "phantomjs-prebuilt": "2.1.7",
    "postcss-loader": "0.13.0",
    "prismjs": "1.5.0",
    "protractor": "3.2.1",
    "rimraf": "2.5.4",
    "serve": "1.4.0",
    "serve-https": "1.5.3",
    "style-loader": "0.13.1",
    "stylint": "1.5.6",
    "stylus": "0.54.5",
    "stylus-loader": "2.3.1",
    "swanky-processor-ngdocs": "2.1.0",
    "swanky-server": "2.9.0",
    "tslint": "4.0.0-dev.1",
    "typescript": "2.0.6",
    "webpack": "2.1.0-beta.25",
    "webpack-dev-server": "2.1.0-beta.9"
  },
  "engines": {
    "node": ">=4.x",
    "npm": ">=3.9.5"
  },
  "private": true
}
